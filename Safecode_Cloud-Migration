******JS******
/*==========Header and Navbar Toggle===========*/
function toggleMenu() {
  document.querySelector('nav').classList.toggle('active');
}

function toggleDropdown(event, dropdownId) {
  event.preventDefault();
  const dropdown = document.getElementById(dropdownId);
  dropdown.classList.toggle('active');
  const parentLi = dropdown.parentElement;
  parentLi.classList.toggle('active');

  // Update the caret icon
  const caret = event.currentTarget.querySelector('.caret');
  if (dropdown.classList.contains('active')) {
    caret.style.transform = 'rotate(180deg)';
  } else {
    caret.style.transform = 'rotate(0deg)';
  }
}

/*================Dropdown====================*/
document.querySelectorAll('.section-header').forEach(header => {
  header.addEventListener('click', () => {
      const content = header.nextElementSibling;
      const symbol = header.querySelector('.toggle-symbol');
      content.style.display = content.style.display === 'none' ? 'block' : 'none';
      symbol.textContent = symbol.textContent === '+' ? '−' : '+';
  });
});

// Initialize all sections as collapsed
document.querySelectorAll('.section-content').forEach(content => {
  content.style.display = 'none';
});
