******JS******
/*==========Header and Navbar Toggle===========*/
function toggleMenu() {
  document.querySelector('nav').classList.toggle('active');
}

function toggleDropdown(event, dropdownId) {
  event.preventDefault();
  const dropdown = document.getElementById(dropdownId);
  dropdown.classList.toggle('active');
  const parentLi = dropdown.parentElement;
  parentLi.classList.toggle('active');

  // Update the caret icon
  const caret = event.currentTarget.querySelector('.caret');
  if (dropdown.classList.contains('active')) {
    caret.style.transform = 'rotate(180deg)';
  } else {
    caret.style.transform = 'rotate(0deg)';
  }
}

/*================Dropdown===================*/
function toggleAccordion(id) {
  var content = document.getElementById(id);
  var symbol = document.getElementById('symbol-' + id);
  var allContents = document.querySelectorAll('.accordion-content');
  var allSymbols = document.querySelectorAll('.symbol');

  allContents.forEach(function (item) {
      if (item !== content) {
          item.classList.remove('open');
      }
  });

  allSymbols.forEach(function (item) {
      if (item !== symbol) {
          item.textContent = '+';
      }
  });

  if (content.classList.contains('open')) {
      content.classList.remove('open');
      symbol.textContent = '+';
  } else {
      content.classList.add('open');
      symbol.textContent = '-';
  }
}
