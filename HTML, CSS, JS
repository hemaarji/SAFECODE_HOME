  overflow-x: hidden;
}
        /*=================Header================*/
/* Header Image */
header img {
  height: 100px;
  width: auto;
  position: relative;
  margin: 10px 20px;
}

/* Header Styling */
header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15px 20px;
  background-color: #f5f5f5; /* Light background color */
  color: #333; /* Dark text color for contrast */
  box-shadow: 0 4px 8px rgba(0,0,0,0.1); /* Subtle shadow */
}

/* Header Links */
header a {
  color: #333;
  text-decoration: none;
}

/* Menu Toggle */
.menuToggle {
  display: none;
  font-size: 30px;
  cursor: pointer;
  color: #333;
  transition: color 0.3s;
}

.menuToggle:hover {
  color: #ff5722; /* Bright color on hover */
}

/* Navigation Styling */
nav {
  display: flex;
  align-items: center;
  flex-grow: 1;
  font-weight: 600;
  
  margin-left: 20px; /* Adjust space between image and nav */
}

.main-nav {
  display: flex;
  list-style: none;
  margin: 0;
  padding: 0;
}

.main-nav li {
  position: relative;
}

.main-nav li a {
  padding: 10px 20px;
  display: block;
  color: #333;
  font-weight: 600;
  font-family: Cambria, Cochin, Georgia, Times, 'Times New Roman', serif;
  text-transform: uppercase;
  transition: background-color 0.3s, color 0.3s;
}

