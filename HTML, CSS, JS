**JS**

function toggleMenu() {
  document.querySelector('nav').classList.toggle('active');
}

function toggleDropdown(event, dropdownId) {
  event.preventDefault();
  const dropdown = document.getElementById(dropdownId);
  dropdown.classList.toggle('active');
  const parentLi = dropdown.parentElement;
  parentLi.classList.toggle('active');

  // Update the caret icon
  const caret = event.currentTarget.querySelector('.caret');
  if (dropdown.classList.contains('active')) {
    caret.style.transform = 'rotate(180deg)';
  } else {
    caret.style.transform = 'rotate(0deg)';
  }
}



var swiper = new Swiper('.swiper-container', {
  slidesPerView: 4,  // Default number of slides per view
  spaceBetween: 30,
  slidesPerGroup: 1,
  loop: true,
  loopFillGroupWithBlank: true,
  pagination: {
      el: '.swiper-pagination',
      clickable: true,
  },
  navigation: {
      nextEl: '.swiper-button-next',
      prevEl: '.swiper-button-prev',
  },
  autoplay: {
      delay: 3000,
      disableOnInteraction: false,
  },
  breakpoints: {
    1024: {
      slidesPerView: 4,  // 4 slides per view for screens wider than 1024px
      spaceBetween: 30,
    },
    768: {
      slidesPerView: 3,  // 2 slides per view for screens between 768px and 1024px
      spaceBetween: 20,
    },
    480: {
      slidesPerView: 1,  // 1 slide per view for screens narrower than 768px
      spaceBetween: 10,
    },
  },
});
