  padding: 10px 15px;
  color: #333;
}

.main-nav li:hover ul {
  display: block;
}

/* Dropdown Container Styling */
.dropdown-container {
  max-height: 800px; /* Increase this value as needed */
  overflow: hidden;
  background-color: #f5f5f5; /* Light background color */
  border-radius: 8px;
  transition: max-height 0.5s ease-in-out;
}

.dropdown-container.active {
  max-height: 600px; /* Adjust based on content */
}

.dropdown-container li:hover a {
  background-color: #ff5722; /* Bright background on hover */
  color: #fff; /* Light text color on hover */
}

