******JS******
function toggleMenu() {
  document.querySelector('nav').classList.toggle('active');
}

function toggleDropdown(event, dropdownId) {
  event.preventDefault();
  const dropdown = document.getElementById(dropdownId);
  dropdown.classList.toggle('active');
  const parentLi = dropdown.parentElement;
  parentLi.classList.toggle('active');

  // Update the caret icon
  const caret = event.currentTarget.querySelector('.caret');
  if (dropdown.classList.contains('active')) {
    caret.style.transform = 'rotate(180deg)';
  } else {
    caret.style.transform = 'rotate(0deg)';
  }
}



/*JOB SECTION*/
function toggleVisibility(jobId) {
  const jobInfo = document.getElementById(jobId);
  
  if (jobInfo.style.display === 'none' || jobInfo.style.display === '') {
    // Hide all job information divs
    const allJobInfos = document.querySelectorAll('.firstjob_info, .secondjob_info, .thirdjob_info, .fourthjob_info, .fifthjob_info, .sixthjob_info, .seventhjob_info, .eighthjob_info, .ninthjob_info, .tenthjob_info, .eleventhjob_info, .twelfthjob_info, .thirteenthjob_info' );
    allJobInfos.forEach(info => {
      info.style.display = 'none';
    });

    // Show the selected job information
    jobInfo.style.display = 'block';
  } else {
    // Hide the selected job information
    jobInfo.style.display = 'none';
  }
}
